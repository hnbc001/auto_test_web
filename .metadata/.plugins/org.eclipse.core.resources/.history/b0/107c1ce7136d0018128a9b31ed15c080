/**
 * @author chihongyuan
 * 支付页正常跳转
 */
package com.gxyj.app;

import org.testng.Reporter;
import org.testng.annotations.Test;

public class App080_PaymentPage_Chihy extends TestBase {

	@Test
	public void title() {
		
		boolean flag = false;
		try {
			locator.clickByUIA("确认支付");
			locator.wait(5);
			flag = locator.elementIsExist("订单支付页", "订单支付页");
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
			Reporter.log("【fail】080订单支付页跳转失败");
		}
		
		checkPoint.equals(flag, true, "【fail】080订单支付页跳转失败");
		checkPoint.result("【pass】080订单支付页跳转成功");
	}
}
