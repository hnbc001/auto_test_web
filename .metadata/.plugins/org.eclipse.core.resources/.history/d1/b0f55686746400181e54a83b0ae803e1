 */

package com.gxyj.app;

import org.testng.Reporter;
import org.testng.annotations.Test;

public class App013_List_Chihy extends TestBase {

	@Test
	public void list() {
		
		locator.clickByUIA("分类");
		locator.clickByUIA("米面杂粮");
		String text = null;
		try {
			text = locator.getTextByUIA("大米");
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
			Reporter.log("【fail】013APP列表页1展示失败");
		}
		checkPoint.equals(text, "大米", "【fail】013APP列表页1展示失败");
		checkPoint.result("【pass】013APP列表页1展示正常");
	}
}
