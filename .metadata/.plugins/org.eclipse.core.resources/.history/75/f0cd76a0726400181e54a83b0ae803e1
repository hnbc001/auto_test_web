 */

package com.gxyj.app;

import org.testng.Reporter;
import org.testng.annotations.Test;

public class App003_Index_Chihy extends TestBase {
	
	@Test
	public void area() {
		
		boolean flag = false;
		try {
			flag = locator.elementIsExist("首页", "地区下拉列表");
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
			Reporter.log("【fail】003首页地区下拉列表展示失败");
		}
		checkPoint.equals(flag, true, "【fail】003首页地区下拉列表展示失败");
		checkPoint.result("【pass】003首页地区下拉列表展示成功");
	}
}
